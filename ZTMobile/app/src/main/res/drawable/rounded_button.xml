<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <corners android:radius="5dp" />
            <stroke android:width="5dp" android:color="#5e7974" />
            <gradient android:angle="-90" android:startColor="#59b300" android:endColor="#408000" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <corners android:radius="5dp" />
            <stroke android:width="5dp" android:color="#5e7974" />
            <gradient android:angle="-90" android:startColor="#4d9900" android:endColor="#336600" />
        </shape>
    </item>
</selector>